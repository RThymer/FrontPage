{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "predeploy": "umi build",
    "deploy": "gh-pages -d dist",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "6.5.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.10.3",
    "umi": "3.3.5"
  },
  "devDependencies": {
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@umijs/test": "3.3.5",
    "lint-staged": "10.0.7",
    "gh-pages": "3.1.0",
    "prettier": "2.2.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "typescript": "4.1.2",
    "yorkie": "2.0.0"
  }
}
